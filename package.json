  "scripts": {
    "module:nest": "nx g @nrwl/nest:lib",
    "prepare": "husky install",
    "compile:contracts": "nx compile smart-contracts",
    "deploy:contracts:testnet": "nx deploy-testnet smart-contracts",
    "deploy:contracts:mainnet": "nx deploy-mainnet smart-contracts",
    "build:client": "nx build client",
    "build:server": "nx build server",
    "build:client:prod": "nx build client --prod",
    "build:server:prod": "nx build server --prod",
    "serve:client": "nx serve client",
    "migrate:db": "nx prisma-migrate prisma",
    "generate:db": "nx prisma-generate prisma",
    "serve:db": "nx restart-db prisma",
    "export:db": "nx export-db prisma",
    "import:db": "nx import-db prisma",
    "serve:db:test": "dotenv -e .env.test -- nx restart-db-test prisma",
    "serve:server": "yarn serve:db & nx serve server",
    "test:server": "nx test server",
    "test:server:watch": "nx test server --watch --no-cache",
    "e2e:server": "yarn serve:db:test && nx e2e server-e2e",
    "e2e:server:watch": "yarn serve:db:test && nx e2e server-e2e --watch --no-cache",
    "remove:db:all": "nx remove-db prisma && nx remove-db-test prisma",
    "lint": "nx workspace-lint && nx lint",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "format": "yarn format:write",
    "update": "nx migrate latest"
  },
  "private": true,
