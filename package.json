{
  "name": "dvs",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Nils Boehm",
  "scripts": {
    "prepare": "husky install",
    "build:client": "nx build client",
    "build:api": "nx build api",
    "build:client:prod": "nx build client --prod",
    "build:api:prod": "nx build api --prod",
    "serve:client": "nx serve client",
    "serve:db": "nx db:restart api-prisma",
    "serve:db:test": "dotenv -e .env.test -- nx db:test:restart api-prisma",
    "serve:api": "yarn serve:db & nx serve api",
    "test:api": "nx test api",
    "test:api:watch": "nx test api --watch --no-cache",
    "e2e:api": "yarn serve:db:test && nx e2e api-e2e",
    "e2e:api:watch": "yarn serve:db:test && nx e2e api-e2e --watch --no-cache",
    "remove:db:all": "nx db:rm api-prisma && nx db:test:rm api-prisma",
    "lint": "nx workspace-lint && nx lint",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "format": "yarn format:write",
    "update": "nx migrate latest"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nrwl/next": "14.5.8",
    "@prisma/client": "^4.2.1",
    "class-validator": "^0.13.2",
    "core-js": "^3.6.5",
    "joi": "^17.6.0",
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "rxjs": "^7.0.0",
    "styled-components": "5.3.5",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/cli": "14.5.8",
    "@nrwl/cypress": "14.5.8",
    "@nrwl/eslint-plugin-nx": "14.5.8",
    "@nrwl/jest": "14.5.8",
    "@nrwl/linter": "14.5.8",
    "@nrwl/nest": "14.5.8",
    "@nrwl/node": "14.5.8",
    "@nrwl/react": "14.5.8",
    "@nrwl/web": "14.5.8",
    "@nrwl/workspace": "14.5.8",
    "@testing-library/react": "13.3.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-is": "17.0.3",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "27.5.1",
    "babel-plugin-styled-components": "1.10.7",
    "cypress": "^10.2.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "~8.15.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.1",
    "jest": "27.5.1",
    "lint-staged": "^13.0.3",
    "nx": "14.5.8",
    "pactum": "^3.1.14",
    "prettier": "^2.6.2",
    "prisma": "^4.2.1",
    "react-test-renderer": "18.2.0",
    "ts-jest": "27.1.4",
    "ts-node": "10.9.1",
    "typescript": "~4.7.2"
  },
  "lint-staged": {
    "*.{js,json,css,scss,md,ts,html,graphql}": [
      "yarn format --uncommitted"
    ]
  }
}
